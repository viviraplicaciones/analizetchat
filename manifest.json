{
  "name": "Analizador de Chats de WhatsApp",
  "short_name": "Analizador WA",
  "description": "Analiza y visualiza tus exportaciones de chats de WhatsApp.",
  "start_url": "./index.html",
  "scope": "./",
  "display": "fullscreen",
  "orientation": "portrait",
  "display_override": [
    "fullscreen",
    "window-controls-overlay",
    "standalone"
  ],
  "background_color": "#f0f2f5",
  "theme_color": "#075e54",
  "icons": [
    {
      "src": "https://raw.githubusercontent.com/viviraplicaciones/analizetchat/refs/heads/main/logo.jpeg",
      "sizes": "192x192",
      "type": "image/jpeg"
    },
    {
      "src": "https://raw.githubusercontent.com/viviraplicaciones/analizetchat/refs/heads/main/logo.jpeg",
      "sizes": "512x512",
      "type": "image/jpeg"
    },
    {
      "src": "https://raw.githubusercontent.com/viviraplicaciones/analizetchat/refs/heads/main/logo.jpeg",
      "sizes": "192x192",
      "type": "image/jpeg",
      "purpose": "any maskable"
    }
  ],
  "share_target": {
    "action": "./share-target-handler/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "files": [
        {
          "name": "file",
          "accept": ["*/*", "text/plain", "application/zip", "application/x-zip-compressed", "application/octet-stream"]
        }
      ]
    }
  }
}